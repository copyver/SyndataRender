<robot name="box">
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="E:/HandleImage/box.STL" scale="0.01 0.01 0.01"/>
      </geometry>
    </visual>
	<!-- 定义箱子的底部 -->
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/> <!-- 调整Z轴的位置，使其处于底部 -->
      <geometry>
        <mesh filename="E:/HandleImage/cemian1.STL" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <!-- 箱子的一侧 -->
    <collision>
      <origin xyz="0 0 0" rpy="-1.5708 0 0"/> <!-- X轴位置保持不变 -->
      <geometry>
        <mesh filename="E:/HandleImage/cemian3.STL" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <!-- 箱子的另一侧 -->
    <collision>
      <origin xyz="0 0 0" rpy="-1.5708 0 0"/> <!-- X轴位置保持不变 -->
      <geometry>
        <mesh filename="E:/HandleImage/cemian3.STL" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <!-- 箱子的前面 -->
    <collision>
      <origin xyz="0 0.3 0" rpy="0 0 0"/> <!-- 调整Y轴的位置，使其处于前面 -->
      <geometry>
        <mesh filename="E:/HandleImage/cemian2.STL" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <!-- 箱子的后面 -->
    <collision>
      <origin xyz="0 -0.3 0" rpy="0 0 0"/> <!-- 调整Y轴的位置，使其处于后面 -->
      <geometry>
        <mesh filename="E:/HandleImage/cemian2.STL" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <!-- 顶部开口，不需要添加碰撞体 -->
  </link>
</robot>